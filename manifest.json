{
  "name": "Straight to You",
  "version": "1.0",
  "description": "Open a link in a new tab and go straight to the video!",
  "background" : {
    "persistent": true,
    "scripts" : ["background.js"]
  },
  "icons": {
    "16" : "images/STY_16.png",
    "48" : "images/STY_48.png",
    "128" : "images/STY_128.png"
  },
  "permissions": ["tabs", "contextMenus", "http://*/*", "https://*/*"],
  "manifest_version" : 2
}
